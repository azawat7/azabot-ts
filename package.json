  "scripts": {
    "clean": "npm run clean --workspaces",
    "build:bot": "npm run build --workspace=bot",
    "build:web": "npm run build --workspace=web",
    "build:types": "npm run build --workspace=@shaw/types",
    "build:database": "npm run build --workspace=@shaw/database",
    "build:utils": "npm run build --workspace=@shaw/utils",
    "build:all": "npm run build:types && npm run build:utils && npm run build:database && npm run build:bot && npm run build:web",
    "build": "npm run build:all",
    "dev:bot": "npm run dev --workspace=bot",
    "dev:web": "npm run dev --workspace=web",
    "start:bot": "npm run start --workspace=bot",
    "start:web": "npm run start --workspace=web"
  },
  "workspaces": [
    "packages/*",
    "bot",
    "web"
  ],
  "devDependencies": {
    "cross-env": "^10.0.0",
    "rimraf": "^6.0.1"
  }
